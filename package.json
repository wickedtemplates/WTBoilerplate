{
  "name": "wickedtemplates",
  "version": "0.0.1",
  "description": "A WickedTemplates boilerplate ready to style your free/purchased templates",
  "main": "wt/setup.js",
  "bin": {
    "compile": "node-sass sass/main.scss public/css/style.css -w"
  },
  "scripts": {
    "postinstall": "node wt/setup.js",
    "compile:sass": "node-sass sass/main.scss public/src/css/style.css -w",
    "compile:less": "lessc less/main.less public/src/css/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wickedtemplates/WTBoilerplate.git"
  },
  "keywords": [
    "wickedtemplates"
  ],
  "author": "Wicked Templates",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wickedtemplates/WTBoilerplate/issues"
  },
  "homepage": "https://github.com/wickedtemplates/WTBoilerplate#readme",
  "devDependencies": {},
  "dependencies": {
    "folder-structure-generator": "^1.0.5",
    "less": "^3.12.2",
    "node-sass": "^4.14.1"
  }
}
